{
  "name": "alchemy-platform",
  "version": "0.1.0",
  "description": "Alchemy by Arkane - AI Content Creation Platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "db:studio": "DATABASE_URL=\"postgresql://postgres:postgres@localhost:5432/alchemy_platform?schema=public\" prisma studio",
    "db:migrate": "DATABASE_URL=\"postgresql://postgres:postgres@localhost:5432/alchemy_platform?schema=public\" prisma migrate dev",
    "db:push": "DATABASE_URL=\"postgresql://postgres:postgres@localhost:5432/alchemy_platform?schema=public\" prisma db push",
    "db:generate": "prisma generate",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "workers": "DATABASE_URL=\"postgresql://postgres:postgres@localhost:5432/alchemy_platform?schema=public\" REDIS_URL=\"redis://:alchemyredis@localhost:6379\" tsx workers/index.ts",
    "test:connections": "DATABASE_URL=\"postgresql://postgres:postgres@localhost:5432/alchemy_platform?schema=public\" REDIS_URL=\"redis://:alchemyredis@localhost:6379\" tsx scripts/test-connections.ts"
  },
  "keywords": [
    "ai",
    "content",
    "marketing",
    "automation"
  ],
  "author": "Arkane",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.1",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.0",
    "@tanstack/react-query": "^5.90.10",
    "axios": "^1.13.2",
    "bullmq": "^5.64.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ioredis": "^5.8.2",
    "next": "^16.0.3",
    "openai": "^6.9.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
